    "helmet": "^8.0.0",
    "http-errors": "^2.0.0",
    "mongoose": "^8.9.5",
    "morgan": "^1.10.0",
    "strftime": "^0.10.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.0.5",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
